{
  "name": "musakorneri",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "deploy": "npm run build && aws s3 sync out/ s3://musakorneri-app --delete && aws cloudfront create-invalidation --distribution-id $(aws cloudformation describe-stacks --stack-name MusakorneriFrontendStack --query 'Stacks[0].Outputs[?OutputKey==`DistributionId`].OutputValue' --output text) --paths '/*'"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.962.0",
    "@aws-sdk/client-dynamodb": "^3.962.0",
    "@aws-sdk/lib-dynamodb": "^3.962.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@reduxjs/toolkit": "^2.11.2",
    "lodash-es": "^4.17.22",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
